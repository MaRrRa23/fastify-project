{
    "root": true,
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2022,
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "rules": {
        "consistent-return": [
            "error",
            {
                "treatUndefinedAsUnspecified": false
            }
        ]
    }
}